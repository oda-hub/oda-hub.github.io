        <rdfs:label>DataProduct</rdfs:label>
    </owl:Class>
    


    <!-- http://odahub.io/ontology#LightCurveList -->


    <owl:Class rdf:about="http://odahub.io/ontology#LightCurveList">
        <rdfs:subClassOf rdf:resource="http://odahub.io/ontology#DataProduct"/>
        <rdfs:label>LightCurveList</rdfs:label>
    </owl:Class>
    


    <!-- http://odahub.io/ontology#ODAAPILightcurveList -->


    <owl:Class rdf:about="http://odahub.io/ontology#ODAAPILightcurveList">
        <rdfs:subClassOf rdf:resource="http://odahub.io/ontology#LightCurveList"/>
