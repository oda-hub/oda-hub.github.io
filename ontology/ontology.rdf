    <!-- http://odahub.io/ontology#AstrophysicalObject -->


    <owl:Class rdf:about="http://odahub.io/ontology#AstrophysicalObject">
        <rdfs:comment>Any Astrophysical object, possibly contained in space and/or time: Galaxy, Gamma-Ray Burst, Universe. Note that an object can be spatially or temporarily contained within another: e.g. a BlazarFlare is an episode of Blazar life, and a SunSpot is on the Sun.</rdfs:comment>
        <rdfs:label>AstrophysicalObject</rdfs:label>
    </owl:Class>
    


    <!-- http://odahub.io/ontology#AstroqueryModule -->


    <owl:Class rdf:about="http://odahub.io/ontology#AstroqueryModule">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>An astroquery module is used internally by workflows to access data (observations, catalogs, etc).</rdfs:comment>
        <rdfs:label>AstroqueryModule</rdfs:label>
    </owl:Class>
    


    <!-- http://odahub.io/ontology#DataProduct -->


    <owl:Class rdf:about="http://odahub.io/ontology#DataProduct">
