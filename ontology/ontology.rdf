    <!-- http://odahub.io/ontology#isRequesting -->


    <owl:ObjectProperty rdf:about="http://odahub.io/ontology#isRequesting">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://odahub.io/ontology#Workflow"/>
        <rdfs:range rdf:resource="http://odahub.io/ontology#AstrophysicalObject"/>
        <rdfs:range rdf:resource="http://odahub.io/ontology#SkyCoordinates"/>
    </owl:ObjectProperty>
    


    <!-- http://odahub.io/ontology#isRequestingAstroObject -->


    <owl:ObjectProperty rdf:about="http://odahub.io/ontology#isRequestingAstroObject">
        <rdfs:subPropertyOf rdf:resource="http://odahub.io/ontology#isRequesting"/>
        <rdfs:range rdf:resource="http://odahub.io/ontology#AstrophysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- http://odahub.io/ontology#isRequestingParameter -->


    <owl:ObjectProperty rdf:about="http://odahub.io/ontology#isRequestingParameter">
        <rdfs:subPropertyOf rdf:resource="http://odahub.io/ontology#isRequesting"/>
    </owl:ObjectProperty>
    


    <!-- http://odahub.io/ontology#isUsing -->


    <owl:ObjectProperty rdf:about="http://odahub.io/ontology#isUsing">
        <rdfs:subPropertyOf rdf:resource="http://odahub.io/ontology#isRequesting"/>
    </owl:ObjectProperty>
    


